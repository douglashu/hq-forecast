<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <stroke
        android:width="1dp"
        android:color="#69D8B0"/>
    <!-- 填充的颜色 -->
    <solid android:color="#FEFEFE"/>
    <!-- 设置按钮的四个角为弧形 -->
    <!-- android:radius 弧形的半径 -->
    <corners
        android:radius="25dp"/>
    <padding
        android:bottom="0dp"
        android:left="12dp"
        android:right="12dp"
        android:top="0dp"/>
</shape>